{
	"name": "filter-search",
	"author": {
		"name": "Vignesh D",
		"email": "vignesh0025@gmail.com"
	},
	"publisher": "vd-vignesh0025",
	"displayName": "Filter Search",
	"description": "Filters the search matches in single panel and jump to the position in main file",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:filter-search.jumptoline",
		"onCommand:filter-search.search",
		"onView:search-view"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"menus": {
			"view/title": [
				{
					"when": "view == search-view ",
					"group": "navigation",
					"command": "filter-search.clearresults"
				}
			]
		},
		"viewsContainers": {
			"panel": [
				{
					"id": "search-view-container",
					"title": "FilterSearch: Search",
					"icon": "$(search)"
				}
			]
		},
		"views": {
			"search-view-container": [
				{
					"id": "search-view",
					"name": "FilterSearch: Results",
					"contextualTitle": "Search Results"
				}
			]
		},
		"commands": [
			{
				"command": "filter-search.search",
				"title": "FilterSearch: Search",
				"icon":{
					"dark": "clear-all",
					"light": "clear-all"
				}
			},
			{
				"command": "filter-search.jumptoline",
				"title": "FilterSearch: Jump to match",
				"enablement": "false"
			},
			{
				"command": "filter-search.clearresults",
				"title": "FilterSearch: Clear Results"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.18.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"ts-loader": "^9.3.1",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"@vscode/test-electron": "^2.1.5"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/vignesh0025/fitler-search.git"
	}
}
